{
    "telemetry.telemetryLevel": "off",
    "workbench.startupEditor": "none",
    "editor.fontFamily": "FiraCode Nerd Font",
    "editor.fontLigatures": false,
    "editor.fontSize": 16,
    "editor.lineNumbers": "relative",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.detectIndentation": true,
    "editor.rulers": [
       80,
       120
    ],
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.macOptionIsMeta": true,
    "terminal.integrated.fontWeight": "normal",
    "terminal.integrated.fontWeightBold": "bold",
    "vim.foldfix": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j","k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["j","k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before":["<leader>", "n"],
            "after":[],
            "commands": [
               {
                    "command": ":nohl"
                }
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys":{
        "<C-a>": false,
        "<C-f>": true,
    },
    "window.title": "${activeEditorMedium}${separator}${rootName}",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "editor.parameterHints.enabled": false,
    "debug.console.fontSize": 16,
    "merge-conflict.diffViewPosition": "Below",
    "extensions.ignoreRecommendations": false,
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "workbench.editor.enablePreview": false,
    "redhat.telemetry.enabled": false,
    "editor.guides.bracketPairs": "active",
    "vs-kubernetes": {
        "vscode-kubernetes.helm-path.mac": "/Users/caccola/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vscode-kubernetes.minikube-path.mac": "/Users/caccola/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.minikube-show-information-expiration": "2023-01-08T04:57:51.014Z"
    },
    "go.toolsManagement.autoUpdate": true,
    "workbench.colorTheme": "Catppuccin Macchiato",
    "editor.quickSuggestions": {
        "other": "on",
        "comments": "off",
        "strings": "off"
    },
    "editor.bracketPairColorization.enabled": false,
    "terminal.integrated.scrollback": 50000,
    "gopls": {
        "ui.semanticTokens": true
    },
    "terminal.integrated.profiles.osx": {
        "bash": null,
        "zsh": {
            "path": "zsh",
            "args": [
                "-l"
            ]
        },
        "fish": null,
        "tmux": null,
        "pwsh": null,
    },
    "terminal.integrated.defaultProfile.osx": "zsh",
    "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.vim"
}
